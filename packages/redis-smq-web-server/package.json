{
  "name": "redis-smq-web-server",
  "version": "9.0.9-next.2",
  "description": "Web server for RedisSMQ Web UI: serves the SPA and hosts or proxies the RedisSMQ REST API.",
  "author": "Weyoss <weyoss@outlook.com>",
  "license": "MIT",
  "keywords": [
    "redis",
    "redis-smq",
    "message-queue",
    "web-server",
    "rest-api",
    "proxy",
    "http",
    "express",
    "spa",
    "dashboard",
    "monitoring"
  ],
  "homepage": "https://github.com/weyoss/redis-smq",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/weyoss/redis-smq.git"
  },
  "bugs": {
    "url": "https://github.com/weyoss/redis-smq/issues"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "scripts/build.sh",
    "dev": "tsc -w & node --watch dist/index.js",
    "test": "../../scripts/test-workspace-esm.sh"
  },
  "bin": {
    "redis-smq-web-server": "./dist/esm/bin/cli.js"
  },
  "dependencies": {
    "commander": "14.0.2",
    "express": "4.21.2",
    "http-proxy-middleware": "3.0.5"
  },
  "devDependencies": {
    "@types/express": "4.17.25"
  },
  "peerDependencies": {
    "@redis/client": "^5",
    "ioredis": "^5",
    "redis-smq": "workspace:^",
    "redis-smq-common": "workspace:^",
    "redis-smq-rest-api": "workspace:^",
    "redis-smq-web-ui": "workspace:^"
  },
  "peerDependenciesMeta": {
    "@redis/client": {
      "optional": true
    },
    "ioredis": {
      "optional": true
    }
  },
  "engineStrict": true,
  "engines": {
    "node": ">=20"
  }

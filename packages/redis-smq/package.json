{
  "name": "redis-smq",
  "version": "9.0.0-next.2",
  "description": "A high-performance, reliable, and scalable message queue for Node.js.",
  "author": "Weyoss <weyoss@protonmail.com>",
  "license": "MIT",
  "keywords": [
    "redis",
    "redis-smq",
    "message-queue",
    "queue",
    "job-queue",
    "task-queue",
    "delayed-jobs",
    "scheduled-jobs",
    "retry",
    "dead-letter",
    "priority-queue",
    "fifo",
    "lifo",
    "nodejs",
    "typescript"
  ],
  "homepage": "https://github.com/weyoss/redis-smq",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/weyoss/redis-smq.git"
  },
  "bugs": {
    "url": "https://github.com/weyoss/redis-smq/issues"
  },
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "redis-smq-common": "workspace:^"
  },
  "dependencies": {
    "cron-parser": "4.9.0",
    "lodash": "4.17.21",
    "uuid": "11.1.0"
  },
  "scripts": {
    "build": "scripts/build.sh",
    "test": "../../scripts/test_workspace_esm.sh",
    "document": "scripts/document.sh"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=20"
  }
}

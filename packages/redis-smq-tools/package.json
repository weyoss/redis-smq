{
  "name": "redis-smq-tools",
  "version": "8.0.0-rc.30",
  "description": "Tools and helpers for developing and testing RedisSMQ",
  "author": "Weyoss <weyoss@protonmail.com>",
  "license": "MIT",
  "keywords": [
    "redis",
    "message queue",
    "message-queue",
    "message",
    "queue",
    "job queue",
    "job-queue",
    "jobs",
    "redis-smq",
    "priority",
    "priority queue",
    "priority-queue",
    "scheduler",
    "broker",
    "message broker",
    "message-broker"
  ],
  "homepage": "https://github.com/weyoss/redis-smq-common",
  "bugs": {
    "url": "https://github.com/weyoss/redis-smq-common/issues"
  },
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "axios": "1.8.2",
    "tar": "7.4.3"
  },
  "scripts": {
    "build": "scripts/build.sh",
    "test": "../../scripts/test_workspace_esm.sh"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/weyoss/redis-smq.git"
  }
}

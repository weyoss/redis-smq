{
  "name": "redis-smq-rest-api",
  "version": "8.3.1",
  "description": "REST API for RedisSMQ: OpenAPI 3 schema and Swagger UI for managing queues, messages, and consumers.",
  "author": "Weyoss <weyoss@protonmail.com>",
  "license": "MIT",
  "keywords": [
    "redis",
    "redis-smq",
    "message-queue",
    "queue",
    "rest",
    "rest-api",
    "http",
    "api",
    "openapi",
    "swagger",
    "koa",
    "monitoring"
  ],
  "homepage": "https://github.com/weyoss/redis-smq",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/weyoss/redis-smq.git"
  },
  "bugs": {
    "url": "https://github.com/weyoss/redis-smq/issues"
  },
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@koa/bodyparser": "6.0.0",
    "@koa/cors": "5.0.0",
    "@koa/router": "14.0.0",
    "@openapi-contrib/json-schema-to-openapi-schema": "3.0.3",
    "ajv": "8.17.1",
    "awilix": "9.0.0",
    "bluebird": "3.7.2",
    "commander": "13.1.0",
    "json-schema": "0.4.0",
    "koa": "3.0.1",
    "koa-mount": "4.0.0",
    "koa-static": "5.0.0",
    "lodash": "4.17.21",
    "swagger-ui-dist": "5.27.1",
    "tmp": "0.2.3",
    "typescript-json-schema": "0.65.1",
    "uuid": "11.1.0"
  },
  "peerDependencies": {
    "redis-smq": "workspace:^",
    "redis-smq-common": "workspace:^"
  },
  "devDependencies": {
    "@types/json-schema": "7.0.15",
    "@types/koa": "3.0.0",
    "@types/koa-mount": "4.0.5",
    "@types/koa-static": "4.0.4",
    "@types/koa__cors": "5.0.0",
    "@types/koa__router": "12.0.4",
    "@types/swagger-ui-dist": "3.30.6",
    "@types/tmp": "0.2.6",
    "openapi-types": "12.1.3"
  },
  "scripts": {
    "schema:gen": "scripts/json-schema-gen.sh",
    "openapi:gen": "scripts/openapi-gen.js",
    "build": "scripts/build.sh",
    "test": "../../scripts/test_workspace_esm.sh"
  },
  "bin": {
    "redis-smq-rest-api": "./dist/esm/bin/cli.js"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=20"
  }
}
